function ChangeJPowerValue()
	local NewValue = JPowerValue
	JumpPowerSlider.Point:TweenPosition(UDim2.new(NewValue/MaxValue, 0, 0.5, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.1, true, nil)
	JumpPowerSlider.Back:TweenSize(UDim2.new(NewValue/MaxValue, 0, 1, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.1, true, nil)
	Player.Character.Humanoid.JumpPower = NewValue
	JumpPowerSlider.JPowerValueLabel.Text = math.floor(NewValue)
end

function SetJPowerFromInput(input)
	if isDragging then
		local Pos
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			Pos = input.Position.X - JumpPowerSlider.AbsolutePosition.X
			if Pos < 0 or Pos == 0 then
				Pos = 0
			end
			if Pos > JumpPowerSlider.AbsoluteSize.X or Pos == JumpPowerSlider.AbsoluteSize.X then
				Pos = JumpPowerSlider.AbsoluteSize.X
			end
			JPowerValue = (Pos/JumpPowerSlider.AbsoluteSize.X) * MaxValue
			ChangeJPowerValue()
		end
	end
end

JumpPowerSlider.MouseButton.MouseButton1Down:Connect(function()
	isDragging = true
end)

-- Use TouchInput to support mobile devices

UserInputService.TouchStarted:Connect(function(touch, processed)
	if not processed and touch.Target == JumpPowerSlider and touch.UserInputType == Enum.UserInputType.Touch then
		isDragging = true
	end
end)

UserInputService.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		isDragging = false
	end
end)

UserInputService.InputChanged:Connect(function(input)
	SetJPowerFromInput(input)
end)

-- WalkSpeed

local SpeedSlider = MainFrame.Home.LocalPlayerFrame.SetSpeed
local MaxValue = 500
local WSpeedValue = 16
SpeedSlider.SpeedValueLabel.Text = WSpeedValue

local isDragging = false

