-- Script Searching

local function filterButtons(input)
	local lowerInput = string.lower(input)

	for _, frame in ipairs(ScriptHolder:GetChildren()) do
		if frame:IsA("Frame") then
			local TitleLabel = frame.Title
			if TitleLabel then
				local Text = string.lower(TitleLabel.Text)
				local match = string.find(Text, lowerInput, 1, true)
				frame.Visible = (match ~= nil)
			end
		end
	end
end

SearchBox.Changed:Connect(function(property)
	if property == "Text" then
		filterButtons(SearchBox.Text)
	end
end)

-- Sliders
-- Gravity

local GravitySlider = MainFrame.Home.LocalPlayerFrame.SetGravity
local MaxValue = 500
local GravityValue = 196.2
GravitySlider.GravityValueLabel.Text = GravityValue

local isDragging = false

function ChangeGravityValue()
	local NewValue = GravityValue
	GravitySlider.Point:TweenPosition(UDim2.new(NewValue/MaxValue, 0, 0.5, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.1, true, nil)
	GravitySlider.Back:TweenSize(UDim2.new(NewValue/MaxValue, 0, 1, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.1, true, nil)
	game.Workspace.Gravity = NewValue
	GravitySlider.GravityValueLabel.Text = math.floor(NewValue)
end

function SetGravityFromInput(input)
	if isDragging then
		local Pos
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			Pos = input.Position.X - GravitySlider.AbsolutePosition.X
			if Pos < 0 or Pos == 0 then
				Pos = 0
			end
			if Pos > GravitySlider.AbsoluteSize.X or Pos == GravitySlider.AbsoluteSize.X then
				Pos = GravitySlider.AbsoluteSize.X
			end
			GravityValue = (Pos/GravitySlider.AbsoluteSize.X) * MaxValue
			ChangeGravityValue()
		end
	end
end

GravitySlider.MouseButton.MouseButton1Down:Connect(function()
	isDragging = true
end)

-- Use TouchInput to support mobile devices

UserInputService.TouchStarted:Connect(function(touch, processed)
	if not processed and touch.Target == GravitySlider and touch.UserInputType == Enum.UserInputType.Touch then
		isDragging = true
	end
end)

UserInputService.InputEnded:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		isDragging = false
	end
end)

UserInputService.InputChanged:Connect(function(input)
	SetGravityFromInput(input)
end)

