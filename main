-- Drag GUI

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

MainFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = MainFrame.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

MainFrame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

-- Page Switching

for _, v in ipairs(Menu:GetChildren()) do
	if v:IsA("ImageButton") then
		v.MouseButton1Click:Connect(function()
			local TweenInfo = TweenInfo.new(0.2)

			local targetPosition = UDim2.new(v.Position.X.Scale, v.Position.X.Offset, v.Position.Y.Scale, v.Position.Y.Offset + 20)
			local tween = TweenService:Create(Menu.SelectionBar, TweenInfo, {Position = targetPosition})
			tween:Play()

			if v:FindFirstChild("Page") then
				v.Page.Value.Visible = true
				v.ImageTransparency = 0
			end	
		end)	
	end	
end

for _, v in ipairs(Menu:GetChildren()) do
	if v:IsA("ImageButton") and v:FindFirstChild("Page") then
		v.MouseButton1Click:Connect(function()
			for _, child in pairs(Menu:GetChildren()) do
				if child ~= v and child:IsA("ImageButton") and child:FindFirstChild("Page") then
					child.Page.Value.Visible = false
					child.ImageTransparency = 0.4
				end
			end
		end)
	end
end

-- Script Execution

-- Create Script Buttons

local function createButton(name, fe, owner, experience, description, code)
	local frame = Instance.new("Frame")
	frame.Parent = ScriptHolder
	frame.ZIndex = 3
	frame.BackgroundColor3 = Color3.fromRGB(0, 25, 38)
	frame.Name = name

	local button = Instance.new("TextButton", frame)
	button.BackgroundTransparency = 1
	button.Text = ""
	button.ZIndex = 5
	button.Size = UDim2.new(1, 0, 1, 0)
	
	-- Store the script code as a custom property of the button
	button:SetAttribute("ScriptCode", code)

	local buttonCorner = Instance.new("UICorner", frame)
	buttonCorner.CornerRadius = UDim.new(0, 8)
	
	local ByLabel = Instance.new("TextLabel", frame)
	ByLabel.Text = "by"
	ByLabel.Size = UDim2.new(0.857, 0, 0.075, 14)
	ByLabel.Position = UDim2.new(0.143, 0, 0.425, 0)
	ByLabel.ZIndex = 4
	ByLabel.BackgroundTransparency = 1
	ByLabel.Font = Enum.Font.SourceSansSemibold
	ByLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	ByLabel.TextScaled = true
	ByLabel.TextXAlignment = Enum.TextXAlignment.Left

	local ScriptTitleLabel = Instance.new("TextLabel", frame)
	ScriptTitleLabel.ZIndex = 4
	ScriptTitleLabel.Text = name
	ScriptTitleLabel.Font = Enum.Font.MontserratBlack
	ScriptTitleLabel.Size = UDim2.new(0.857, 0, 0.162, 14)
	ScriptTitleLabel.Position = UDim2.new(0.071, 0, 0.063, 0)
	ScriptTitleLabel.TextScaled = true
	ScriptTitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	ScriptTitleLabel.BackgroundTransparency = 1
	ScriptTitleLabel.Name = "Title"

	local RunButton = SelectedFrame.RunButton

	RunButton.MouseButton1Click:Connect(function()
		if ScriptTitle.Text == ScriptTitleLabel.Text then
				loadstring(code)()
			end
	end)

	
	local CreatorLabel = Instance.new("TextLabel", frame)
	CreatorLabel.ZIndex = 4
	CreatorLabel.Size = UDim2.new(0.857, 0, 0.05, 14)
	CreatorLabel.Position = UDim2.new(0.071, 0, 0.675, 0)
	CreatorLabel.BackgroundTransparency = 1
	CreatorLabel.Text = owner
	CreatorLabel.TextScaled = true
	CreatorLabel.TextColor3 = Color3.fromRGB(255, 255, 255)

	button.MouseButton1Click:Connect(function()
		SelectedFrame.ScriptTitle.Text = name
		SelectedFrame.DescriptionHolder.DescriptionLabel.Text = "Description - "..description
		SelectedFrame.GameValue.Text = experience
		SelectedFrame.OwnerValue.Text = owner
		if fe == true then
			SelectedFrame.FeValue.Text = "Yes"
			SelectedFrame.FeValue.TextColor3 = Color3.fromRGB(0, 255, 0)
		elseif fe == false then
			SelectedFrame.FeValue.Text = "No"
			SelectedFrame.FeValue.TextColor3 = Color3.fromRGB(255, 0, 0)
		elseif fe == "Partly" then
			SelectedFrame.FeValue.Text = "Partly"
			SelectedFrame.FeValue.TextColor3 = Color3.fromRGB(255, 255, 255)
		elseif fe == "SS" then
			SelectedFrame.FeValue.Text = "ServerSide"
			SelectedFrame.FeValue.TextColor3 = Color3.fromRGB(0, 255, 255)
			
		end
	end)
end

